<%=form_for(@trip) do |f| %>
    <%=render 'shared/error_messages', object:f.object%>
    
    <%= f.label :company_name %>
    <%= f.text_field :company_name, class: 'form-control' %>

    <%= f.label :starting_station %>
    <%= f.text_field :starting_station, class: 'form-control' %>

    <%= f.label :destination %>
    <%= f.text_field :destination, class: 'form-control' %>

    <%= f.label :departure %>
    <%= f.text_field :departure, class: 'form-control' %>

    <%= f.label :arrival %>
    <%= f.text_field :arrival, class: 'form-control' %>

    <%= f.label :remaining_tickets %>
    <%= f.text_field :remaining_tickets, class: 'form-control' %>

    <%=f.submit "Post", class:"btn btn-primary"%>
<%end%>